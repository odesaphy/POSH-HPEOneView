                <ScriptBlock>

                if (-not [System.String]::IsNullOrWhiteSpace($_.subnetUri)) { '{0}' -f (Send-OVRequest -Uri $_.subnetUri -Hostname $_.ApplianceConnection.Name).networkId }
                else { 'None' }

                </ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>Purpose</Label>
                <PropertyName>purpose</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Smartlink</Label>
