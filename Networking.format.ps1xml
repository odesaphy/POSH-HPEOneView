              </TableColumnItem>
              <TableColumnItem>
                <ScriptBlock>
                  $hostname = $_.ApplianceConnection.Name
                  "{0}" -f ($_.enclosureUris | % { (send-OVrequest $_ -hostname $hostname).name } )
